{
  "name": "@stabgan/openrouter-mcp-multimodal",
  "version": "1.2.1",
  "description": "MCP server for OpenRouter providing text chat and image analysis tools",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "openrouter-multimodal": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "openrouter",
    "ai",
    "llm",
    "vision",
    "image-analysis",
    "modelcontextprotocol"
  ],
  "author": "stabgan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stabgan/openrouter-mcp-multimodal.git"
  },
  "bugs": {
    "url": "https://github.com/stabgan/openrouter-mcp-multimodal/issues"
  },
  "homepage": "https://github.com/stabgan/openrouter-mcp-multimodal#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.89.1",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/sharp": "^0.32.0",
    "shx": "^0.3.4",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "uri-js": "npm:uri-js-replace",
    "whatwg-url": "^14.1.0"
  }
}
